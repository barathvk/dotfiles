Xcursor.size: 16

! ROFI

rofi.lines: 3
rofi.font: Ubuntu 28
rofi.padding: 200
rofi.location: 0
rofi.yoffset: -200
rofi.color-enabled: true
rofi.color-window: #1D2021, #5F819D, #1D2021
rofi.color-normal: #1D2021, #A5B6BC, #1D2021, #5F819D, #C5C8C6
rofi.color-urgent: #1D2021, #A5B6BC, #1D2021, #5F819D, #C5C8C6
rofi.color-active: #1D2021, #A5B6BC, #1D2021, #5F819D, #C5C8C6
rofi.terminal: urxvt
rofi.levenshtein-sort: false

! URXVT
URxvt.font: xft:Ubuntu Mono:size=10
URxvt.scrollBar: false
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight
URxvt*letterSpace: -2

! Base16 OceanicNext
! Scheme: https://github.com/voronianski/oceanic-next-color-scheme

